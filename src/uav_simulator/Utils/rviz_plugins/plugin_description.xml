<library path="rviz_plugins">
  <class name="rviz_plugins/Goal3DTool"
         type="rviz_plugins::Goal3DTool"
         base_class_type="rviz_common::Tool">
    <description>
      Tool for setting 3D goal
    </description>
  </class>

  <class name="rviz_plugins/ProbMapDisplay"
         type="rviz_plugins::ProbMapDisplay"
         base_class_type="rviz_common::Display">
    <description>
      Display of -inf +inf probabilistic occupancy grid map
    </description>
  </class>

  <class name="rviz_plugins/GameLikeInput"
         type="rviz_plugins::GameLikeInput"
         base_class_type="rviz_common::Tool">
    <description>
      game usage
    </description>
  </class>

  <class name="rviz_plugins/AerialMapDisplay"
         type="rviz_plugins::AerialMapDisplay"
         base_class_type="rviz_common::Display">
    <description>
      Display of aerial map
    </description>
  </class>  

  <class name="rviz_plugins/MultiProbMapDisplay"
         type="rviz_plugins::MultiProbMapDisplay"
         base_class_type="rviz_common::Display">
    <description>
      Display of multiple -inf +inf probabilistic occupancy grid map
    </description>
  </class>

</library>